<?xml version="1.0" encoding="utf-8"?>
<resources>
	<string name="app_name">IQNWSAlerts</string>
	<string name="menu_prefs">Settings</string>
	<string name="menu_about">About</string>

	<string name="about_summary">This application came to me one day following a
		Skywarn briefing by a local NWS meteorologist. Some NWS offices
		started issuing alerts recently where the alert area is related to
		storm characteristics rather than just the counties affected. This
		application takes that information and your current location to
		determine whether an alert is active for your location.
	</string>

	<string name="min_severity">Minimum severity to notify</string>
	<string name="severity">Severity of the alert</string>
	<string-array name="severity_levels">
		<item>Unknown</item>
		<item>Minor</item>
		<item>Moderate</item>
		<item>Severe</item>
		<item>Extreme</item>
	</string-array>

	<string name="min_certainty">Minimum certainty to notify</string>
	<string name="certainty">Certainty of the alert</string>
	<string-array name="certainty_levels">
		<item>Unknown</item>
		<item>Unlikely</item>
		<item>Possible</item>
		<item>Likely</item>
		<item>Observed</item>
	</string-array>

	<string name="min_urgency">Minimum urgency to notify</string>
	<string name="urgency">Urgency of the alert</string>
	<string-array name="urgency_levels">
		<item>Unknown</item>
		<item>Past</item>
		<item>Future</item>
		<item>Expected</item>
		<item>Immediate</item>
	</string-array>
</resources>